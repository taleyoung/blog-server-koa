{
  "apps": [
    {
      "name": "server-koa",
      "interpreter": "node_modules/.bin/ts-node",
      "script": "index.ts",
      "cwd": "./",
      "watch": [
        "config",
        "controllers",
        "models",
        "routers",
        "services",
        "utils"
      ],
      "error_file": "./logs/app-err.log",
      "out_file": "./logs/app-out.log",
      "log_date_format": "YYYY-MM-DD HH:mm Z"
    }
  ]
}
